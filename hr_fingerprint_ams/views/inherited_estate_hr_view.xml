<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Inherited View -->
        <record id="inherited_estate_attendance_code_view_form" model="ir.ui.view">
            <field name="name">estate.attendance_code_view_form.inherit</field>
            <field name="model">estate.hr.attendance</field>
            <field name="inherit_id" ref="estate.attendance_code_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@id='att_code']" position="inside">
                    <field name="fingerprint"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>